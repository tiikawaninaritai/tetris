    <span class="hljs-string">'/images/icon-192.png'</span>,
    <span class="hljs-string">'/images/icon-512.png'</span>
];

<span class="hljs-comment">// Install event: cache all essential assets</span>
self.addEventListener(<span class="hljs-string">'install'</span>, <span class="hljs-function"><span class="hljs-params">event</span> =&gt;</span> {
    event.waitUntil(
