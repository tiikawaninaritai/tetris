        <span class="hljs-keyword">case</span> <span class="hljs-string">'ArrowUp'</span>: <span class="hljs-comment">// Rotate</span>
            <span class="hljs-keyword">const</span> shape = currentTetromino.shape;
            <span class="hljs-keyword">const</span> newShape = shape[<span class="hljs-number">0</span>].map(<span class="hljs-function">(<span class="hljs-params">_, colIndex</span>) =&gt;</span> shape.map(<span class="hljs-function"><span class="hljs-params">row</span> =&gt;</span> row[colIndex]).reverse());
            <span class="hljs-keyword">const</span> originalCol = currentTetromino.col;
            <span class="hljs-keyword">let</span> offset = <span class="hljs-number">1</span>;
            
            <span class="hljs-keyword">if</span> (!isCollision({ ...currentTetromino, <span class="hljs-attr">shape</span>: newShape }, currentTetromino.row, currentTetromino.col)) {
                currentTetromino.shape = newShape;
            } <span class="hljs-keyword">else</span> {
