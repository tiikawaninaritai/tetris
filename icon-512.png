                    currentTetromino.col -= offset * <span class="hljs-number">2</span>;
                    <span class="hljs-keyword">if</span>(isCollision({ ...currentTetromino, <span class="hljs-attr">shape</span>: newShape }, currentTetromino.row, currentTetromino.col)) {
                        currentTetromino.col = originalCol;
                    } <span class="hljs-keyword">else</span> {
