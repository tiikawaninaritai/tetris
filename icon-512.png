<span class="hljs-comment">// Fetch event: serve assets from cache first</span>
self.addEventListener(<span class="hljs-string">'fetch'</span>, <span class="hljs-function"><span class="hljs-params">event</span> =&gt;</span> {
    event.respondWith(
