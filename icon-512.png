});

<span class="hljs-comment">// „Ç≤„Éº„É†ÈñãÂßã (Start Game)</span>
<span class="hljs-function"><span class="hljs-keyword">function</span> <span class="hljs-title">startGame</span>(<span class="hljs-params"></span>) </span>{
    board = <span class="hljs-built_in">Array</span>.from({ <span class="hljs-attr">length</span>: ROWS }, <span class="hljs-function">() =&gt;</span> <span class="hljs-built_in">Array</span>(COLS).fill(<span class="hljs-number">0</span>));
